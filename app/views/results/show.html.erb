<div class="page-header">
  <%= link_to results_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_result_path(@result), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show result</h1>
</div>

<dl class="dl-horizontal">
  <dt>Exam:</dt>
  <dd><%= @result.exam_id %></dd>

  <dt>Student:</dt>
  <dd><%= @result.student_id %></dd>

  <dt>Setion:</dt>
  <dd><%= @result.setion_id %></dd>

  <dt>Subject:</dt>
  <dd><%= @result.subject_id %></dd>

  <dt>Result:</dt>
  <dd><%= @result.result %></dd>

  <dt>Date:</dt>
  <dd><%= @result.date %></dd>

  <dt>Remark:</dt>
  <dd><%= @result.remark %></dd>

</dl>
