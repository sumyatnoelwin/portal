<%= simple_form_for(@result) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :exam_id %>
    <%= f.input :student_id %>
    <%= f.input :setion_id %>
    <%= f.input :subject_id %>
    <%= f.input :result %>
    <%= f.input :date %>
    <%= f.input :remark %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
