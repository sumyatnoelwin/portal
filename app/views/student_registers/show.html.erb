<div class="page-header">
  <%= link_to student_registers_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_student_register_path(@student_register), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show student_register</h1>
</div>

<dl class="dl-horizontal">
  <dt>Student:</dt>
  <dd><%= @student_register.student.student_name %></dd>

  <dt>Section:</dt>
  <dd><%= @student_register.section.section_name %></dd>

  <dt>Tution fees:</dt>
  <dd><%= @student_register.tution_fees %></dd>

  <dt>Exam fees:</dt>
  <dd><%= @student_register.exam_fees %></dd>

  <dt>Register date:</dt>
  <dd><%= @student_register.register_date %></dd>

  <dt>Complete tf:</dt>
  <dd><%= @student_register.complete_tf %></dd>

  <dt>Complete ef:</dt>
  <dd><%= @student_register.complete_ef %></dd>

  <dt>Staff:</dt>
  <dd><%= @student_register.staff.staff_name %></dd>

  <dt>Remark:</dt>
  <dd><%= @student_register.remark %></dd>

</dl>
