<%= simple_form_for(@student_register) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :student_id %>
    <%= f.input :section_id %>
    <%= f.input :tution_fees %>
    <%= f.input :exam_fees %>
    <%= f.input :register_date %>
    <%= f.input :complete_tf %>
    <%= f.input :complete_ef %>
    <%= f.input :staff_id %>
    <%= f.input :remark %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
