<%= simple_form_for(@student_register) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs"> 
    <%= f.input :student_id %>
    <%= f.input :section_id, :as => :select, :collection=> Section.all.map { |s| [s.section_name,s.id]  } %>
    <%= f.input :tution_fees %>
    <%= f.input :exam_fees %>
    <%= f.input :complete_tf, :label => "Complete Tution Fees?" %>
    <%= f.input :complete_ef, :label => "Complete Education Fees?" %>
    <%= f.input :remark %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>


<script type="text/javascript">
  $(document).ready(function(){
    $('.datepicker').datepicker({format: 'dd-mm-yyyy'});
  });
</script>