<div class="page-header">
  <%= link_to timetables_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_timetable_path(@timetable), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show timetable</h1>
</div>

<dl class="dl-horizontal">
  <dt>Section:</dt>
  <dd><%= @timetable.section.section_name %></dd>

  <dt>Subject:</dt>
  <dd><%= @timetable.subject.subject_name %></dd>

  <dt>Room:</dt>
  <dd><%= @timetable.room.no %></dd>

  <dt>Lecturer:</dt>
  <dd><%= @timetable.lecturer.lecturer_name %></dd>

  <dt>Start time:</dt>
  <dd><%= @timetable.start_time %></dd>

  <dt>End time:</dt>
  <dd><%= @timetable.end_time %></dd>

  <dt>Description:</dt>
  <dd><%= @timetable.description %></dd>

  <dt>Staff:</dt>
  <dd><%= @timetable.staff.staff_name %></dd>

</dl>
