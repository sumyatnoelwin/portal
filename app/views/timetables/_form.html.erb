<%= simple_form_for(@timetable) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :section_id %>
    <%= f.input :subject_id %>
    <%= f.input :room_id %>
    <%= f.input :lecturer_id %>
    <%= f.input :start_time %>
    <%= f.input :end_time %>
    <%= f.input :description %>
    <%= f.input :staff_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
