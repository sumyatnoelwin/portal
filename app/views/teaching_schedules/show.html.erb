<div class="page-header">
  <%= link_to teaching_schedules_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_teaching_schedule_path(@teaching_schedule), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show teaching_schedule</h1>
</div>

<dl class="dl-horizontal">
  <dt>Lecturer:</dt>
  <dd><%= @teaching_schedule.lecturer.lecturer_name %></dd>

  <dt>Section:</dt>
  <dd><%= @teaching_schedule.section.section_name %></dd>

  <dt>Subject:</dt>
  <dd><%= @teaching_schedule.subject.subject_name %></dd>

  <dt>Start date:</dt>
  <dd><%= @teaching_schedule.start_date.strftime("%d-%m-%Y") %></dd>

  <dt>End date:</dt>
  <dd><%= @teaching_schedule.end_date.strftime("%d-%m-%Y") %></dd>

  <dt>Total:</dt>
  <dd><%= @teaching_schedule.total %></dd>

  <dt>Remark:</dt>
  <dd><%= @teaching_schedule.remark %></dd>

  <dt>Description:</dt>
  <dd><%= @teaching_schedule.description %></dd>

</dl>
